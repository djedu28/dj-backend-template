{
    "name": "dj-backend-template",
    "template": "dj-backend-template",
    "version": "2.2.1-vercel",
    "description": "Uma referencia para criar (futuro's) backend API com NodeJS, seguindo as melhores boas praticas que aprendi durante os anos de desenvolvimento",
    "main": "api/server.js",
    "author": "Eduardo Santos (@DjEdu28)",
    "author-template": "Eduardo Santos (@DjEdu28)",
    "license": "MIT",
    "private": true,
    "repository": "https://github.com/djedu28/dj-backend-template",
    "repository-template": "https://github.com/djedu28/dj-backend-template",
    "scripts": {
        "att": "yarn upgrade-interactive --latest",
        "dev": "(cls||clear) && nodemon -r dotenv/config ./api/server.js",
        "start": "node ./api/server.js",
        "__build": "yarn && yarn init"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.1.4",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.11.1",
        "sequelize": "^6.32.0",
        "winston": "^3.9.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "eslint": "^8.42.0",
        "nodemon": "^2.0.20",
        "sequelize-cli": "^6.6.0",
        "sqlite3": "^5.1.6",
        "vercel": "^31.0.3"
    },
    "keywords": [
        "nodejs",
        "backend",
        "api",
        "express",
        "sequelize",
        "database",
        "best practices",
        "djedu28"
    ]
}
